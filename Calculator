print("Enter the first number")
var1= input()
print("Enter the second number")
var2 = input()
print("Sum of these two number" ,int(var1)+int(var2))
print ("Subtract of these two number" , int(var1)-int(var2))
print("Multiply of these two number" , int(var1)*int(var2))
print("Division of these two number", int(var1) / int(var2))
print("Square of thes two number ", int(var1)**int(var2))
